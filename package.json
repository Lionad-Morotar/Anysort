{
  "name": "anysort",
  "description": "Powerful array sorting, with zero dependencies.",
  "version": "2.0.0",
  "homepage": "https://github.com/Lionad-Morotar/anysort",
  "license": "MIT",
  "main": "build/index.min.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c | cross-env NODE_ENV=development rollup -c",
    "test": "cross-env NODE_ENV=production mocha ./test/index",
    "watch:test": "npm-watch test",
    "preversion": "npm run build && npm test",
    "version": "git add -A build",
    "postversion": "git push && git push --tags"
  },
  "watch": {
    "test": {
      "patterns": [
        "build",
        "test"
      ],
      "runOnChangeOnly": true,
      "extensions": "js",
      "quiet": true
    }
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-node-resolve": "^13.2.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/node": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "mocha": "^2.5.3",
    "npm-watch": "^0.7.0",
    "rollup": "^2.38.5",
    "rollup-plugin-uglify": "^6.0.4",
    "should": "^8.4.0",
    "tslib": "^2.1.0",
    "typescript": "^4.6.3"
  },
  "keywords": [
    "any",
    "array",
    "multiple",
    "nested",
    "object",
    "order",
    "properties",
    "property",
    "sort",
    "sorting",
    "sorter"
  ]
}
