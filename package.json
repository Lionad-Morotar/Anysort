{
  "name": "anysort",
  "description": "Powerful array sorting, with zero dependencies.",
  "version": "1.4.1",
  "homepage": "https://github.com/Lionad-Morotar/anysort",
  "license": "MIT",
  "main": "build/index.min.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development rollup -c -w",
    "dev": "npm start",
    "build": "cross-env NODE_ENV=production rollup -c",
    "test": "cross-env NODE_ENV=production mocha ./test/index",
    "watch:test": "npm-watch test"
  },
  "watch": {
    "test": {
      "patterns": [
        "build",
        "test"
      ],
      "runOnChangeOnly": true,
      "extensions": "js",
      "quiet": true
    }
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "mocha": "^2.5.3",
    "npm-watch": "^0.7.0",
    "rollup": "^2.38.5",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-uglify": "^6.0.4",
    "should": "^8.4.0",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "keywords": [
    "any",
    "arr",
    "array",
    "asc",
    "ascend",
    "desc",
    "descend",
    "element",
    "elements",
    "get",
    "multiple",
    "nested",
    "obj",
    "object",
    "order",
    "ordered",
    "dot",
    "path",
    "prop",
    "properties",
    "property",
    "sort",
    "sorted",
    "sorting"
  ]
}
